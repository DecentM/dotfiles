#!/bin/false
# shellcheck shell=sh

setopt LOCAL_OPTIONS NO_NOTIFY NO_MONITOR

PATH="$PATH:$HOME/bin"
export PATH

DOTFILES_BASEDIR="$(chezmoi source-path)/.."

. "$DOTFILES_BASEDIR/lib/instant-prompt.zsh"
. "$DOTFILES_BASEDIR/lib/utils.zsh"
. "$DOTFILES_BASEDIR/lib/zplug.zsh"
. "$DOTFILES_BASEDIR/lib/completion.zsh"
. "$DOTFILES_BASEDIR/lib/aliases.zsh"
. "$DOTFILES_BASEDIR/lib/bindkeys.zsh"
. "$DOTFILES_BASEDIR/lib/asdf.zsh"
. "$DOTFILES_BASEDIR/lib/zoxide.zsh"
. "$DOTFILES_BASEDIR/lib/history.zsh"
. "$DOTFILES_BASEDIR/lib/p10k.zsh"
