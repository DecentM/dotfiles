#!/bin/false
# shellcheck shell=sh

setopt LOCAL_OPTIONS NO_NOTIFY NO_MONITOR

PATH="$PATH:$HOME/bin"
export PATH

DOTFILES_BASEDIR="$(chezmoi target-path)"

. "$DOTFILES_BASEDIR/.dotfiles/zsh/instant-prompt.zsh"
. "$DOTFILES_BASEDIR/.dotfiles/zsh/utils.zsh"
. "$DOTFILES_BASEDIR/.dotfiles/zsh/zplug.zsh"
. "$DOTFILES_BASEDIR/.dotfiles/zsh/completion.zsh"
. "$DOTFILES_BASEDIR/.dotfiles/zsh/aliases.zsh"
. "$DOTFILES_BASEDIR/.dotfiles/zsh/bindkeys.zsh"
. "$DOTFILES_BASEDIR/.dotfiles/zsh/asdf.zsh"
. "$DOTFILES_BASEDIR/.dotfiles/zsh/zoxide.zsh"
. "$DOTFILES_BASEDIR/.dotfiles/zsh/history.zsh"
. "$DOTFILES_BASEDIR/.dotfiles/zsh/p10k.zsh"
