{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg_primary": "#312e2b",
    "bg_secondary": "#262421",
    "bg_tertiary": "#272522",
    "bg_element": "#3d3a37",
    "bg_selection": "#4a4745",

    "fg_primary": "#f0f0f0",
    "fg_secondary": "#e8e7e6",
    "fg_muted": "#c7c6c5",
    "fg_dark": "#464341",

    "brand_green": "#5d9948",
    "board_light": "#ebecd0",
    "board_dark": "#739552",
    "move_highlight": "#ffff33",

    "green_light": "#81b64c",
    "green_dark": "#4e7837",
    "blue": "#5c9ac4",
    "blue_dark": "#4a7fa8",
    "red": "#c94f4f",
    "red_light": "#e06666",
    "orange": "#e89f4c",
    "orange_dark": "#c68a3c",
    "gold": "#d4a843",
    "purple": "#9370db",

    "border_default": "#4a4745",
    "border_subtle": "#3d3a37"
  },
  "theme": {
    "primary": "brand_green",
    "secondary": "board_dark",
    "accent": "move_highlight",

    "error": "red",
    "warning": "orange",
    "success": "green_light",
    "info": "blue",

    "text": "fg_primary",
    "textMuted": "fg_muted",

    "background": "bg_primary",
    "backgroundPanel": "bg_secondary",
    "backgroundElement": "bg_element",

    "border": "border_default",
    "borderActive": "brand_green",
    "borderSubtle": "border_subtle",

    "diffAdded": "green_light",
    "diffRemoved": "red",
    "diffContext": "fg_muted",
    "diffHunkHeader": "brand_green",
    "diffHighlightAdded": "board_light",
    "diffHighlightRemoved": "red_light",
    "diffAddedBg": "#81b64c",
    "diffRemovedBg": "#c94f4f",
    "diffContextBg": "bg_primary",
    "diffLineNumber": "fg_muted",
    "diffAddedLineNumberBg": "#81b64c",
    "diffRemovedLineNumberBg": "#c94f4f",

    "markdownText": "fg_primary",
    "markdownHeading": "board_light",
    "markdownLink": "blue",
    "markdownLinkText": "brand_green",
    "markdownCode": "board_light",
    "markdownBlockQuote": "fg_muted",
    "markdownEmph": "gold",
    "markdownStrong": "fg_secondary",
    "markdownHorizontalRule": "border_default",
    "markdownListItem": "brand_green",
    "markdownListEnumeration": "brand_green",
    "markdownImage": "blue",
    "markdownImageText": "brand_green",
    "markdownCodeBlock": "fg_secondary",

    "syntaxComment": "fg_muted",
    "syntaxKeyword": "brand_green",
    "syntaxFunction": "blue",
    "syntaxVariable": "board_light",
    "syntaxString": "green_light",
    "syntaxNumber": "orange",
    "syntaxType": "gold",
    "syntaxOperator": "fg_secondary",
    "syntaxPunctuation": "fg_muted"
  }
}
